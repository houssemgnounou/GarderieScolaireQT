         <string notr="true">/* Apply styles to QTableView */
QTableView {
    background-color: #fff; /* White background */
    border: 1px solid #ddd; /* Light gray border */
    border-radius: 8px; /* Rounded corners */
}

/* Apply styles to table headers */
QHeaderView::section {
    background-color: #f5f5f5; /* Light gray background */
    color: #333; /* Dark text color */
    padding: 8px; /* Padding inside the header cells */
    border: none; /* No border */
    border-top: 1px solid #ddd; /* Top border for separation */
}

/* Apply styles to table cells */
QTableView QTableCornerButton::section,
QTableView QTableCornerButton::section:checked {
    background-color: #f5f5f5; /* Light gray background for corner cell */
    border: none;
}

QTableView::item {
    padding: 10px; /* Padding inside the cells */
    border: none; /* No border */
}

/* Apply alternate row colors for better readability */
QTableView::item:nth-child(odd) {
    background-color: #f9f9f9; /* Very light gray background */
}

/* Apply hover styles for cells */
QTableView::item:hover {
    background-color: #e8e8e8; /* Slightly darker background on hover */
}

/* Apply selection styles for cells */
QTableView::item:selected {
    background-color: #1abc9c; /* Teal background on selection */
    color: white; /* White text color on selection */
}

/* Apply custom scroll bar style */
QTableView QScrollBar:vertical {
    border: none;
    background: transparent;
    width: 10px; /* Adjust the width as needed */
}

QTableView QScrollBar:horizontal {
    border: none;
    background: transparent;
    height: 10px; /* Adjust the height as needed */
}

QTableView QScrollBar::handle:vertical,
QTableView QScrollBar::handle:horizontal {
    background: #ccc; /* Scroll bar handle color */
    border-radius: 5px; /* Rounded handle shape */
}

QTableView QScrollBar::add-page:vertical,
QTableView QScrollBar::sub-page:vertical,
QTableView QScrollBar::add-page:horizontal,
QTableView QScrollBar::sub-page:horizontal {
    background: none;
}

QTableView QScrollBar::add-line:vertical,
QTableView QScrollBar::sub-line:vertical,
QTableView QScrollBar::add-line:horizontal,
QTableView QScrollBar::sub-line:horizontal {
    background: none;
}
